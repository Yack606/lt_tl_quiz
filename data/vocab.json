    {
      "id": "2-01",
      "chapter": 2,
      "lt": "Kaip sekasi?",
      "tl": "Kamusta?"
    },
    {
      "id": "2-02",
      "chapter": 2,
      "lt": "Ačiū, gerai! O kaip jums?",
      "tl": "Salamat, mabuti! Kayo po?"
    },
    {
      "id": "2-03",
      "chapter": 2,
      "lt": "Ačiū, gerai! O kaip tau?",
      "tl": "Salamat, mabuti! Ikaw?"
    },
    {
      "id": "2-04",
      "chapter": 2,
      "lt": "Puikiai!",
      "tl": "Magaling!"
    },
    {
      "id": "2-05",
      "chapter": 2,
      "lt": "Labai gerai!",
      "tl": "Napakabuti!"
    },
    {
      "id": "2-06",
      "chapter": 2,
      "lt": "Šiaip sau.",
      "tl": "Medyo lang / so‑so."
    },
    {
      "id": "2-07",
      "chapter": 2,
      "lt": "Blogai.",
      "tl": "Masama ang lagay."
    },
    {
      "id": "2-08",
      "chapter": 2,
      "lt": "Iš kur jūs esate?",
      "tl": "Taga saan po kayo?"
    },
    {
      "id": "2-09",
      "chapter": 2,
      "lt": "Aš esu iš Vokietijos.",
      "tl": "Ako ay mula sa Alemanya."
    },
    {
      "id": "2-10",
      "chapter": 2,
      "lt": "Iš kur tu esi?",
      "tl": "Taga saan ka?"
    },
    {
      "id": "2-11",
      "chapter": 2,
      "lt": "Aš esu iš Amerikos, iš Niujorko.",
      "tl": "Ako ay mula sa Amerika, mula sa New York."
    },
    {
      "id": "2-12",
      "chapter": 2,
      "lt": "Čia mano draugas Paulius.",
      "tl": "Ito ang kaibigan kong si Paulius."
    },
    {
      "id": "2-13",
      "chapter": 2,
      "lt": "Malonu.",
      "tl": "Ikinagagalak."
    },
    {
      "id": "2-14",
      "chapter": 2,
      "lt": "Ar tu supranti lietuviškai?",
      "tl": "Naiintindihan mo ba ang Litwano?"
    },
    {
      "id": "2-15",
      "chapter": 2,
      "lt": "Taip, truputį suprantu.",
      "tl": "Oo, kaunti lang ang naiintindihan ko."
    },
    {
      "id": "2-16",
      "chapter": 2,
      "lt": "Ar tu kalbi angliškai?",
      "tl": "Nagsasalita ka ba ng Ingles?"
    },
    {
      "id": "2-17",
      "chapter": 2,
      "lt": "Ne, nekalbu.",
      "tl": "Hindi, hindi ako nagsasalita."
    },
    {
      "id": "2-18",
      "chapter": 2,
      "lt": "Aš kalbu lietuviškai ir angliškai.",
      "tl": "Nagsasalita ako ng Litwano at Ingles."
    },
    {
      "id": "2-19",
      "chapter": 2,
      "lt": "Kaip jūs kalbate?",
      "tl": "Anong mga wika ang sinasalita ninyo?"
    },
    {
      "id": "2-20",
      "chapter": 2,
      "lt": "Ar aišku?",
      "tl": "Malinaw ba?"
    },
    {
      "id": "2-21",
      "chapter": 2,
      "lt": "Taip, aišku. / Ne, neaišku.",
      "tl": "Oo, malinaw. / Hindi, hindi malinaw."
    },
    {
      "id": "2-22",
      "chapter": 2,
      "lt": "Prašom pakartoti!",
      "tl": "Pakisabi ulit!"
    },
    {
      "id": "2-23",
      "chapter": 2,
      "lt": "Prašom kalbėti lėčiau!",
      "tl": "Pakibagalan ang pagsasalita!"
    },
